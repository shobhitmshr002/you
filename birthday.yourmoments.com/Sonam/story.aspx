    function checkNewBaby(source, args)
    {        
        var day1 = document.getElementById('ctl00_ModuleContentPlaceHolder_ddlDate1Day').value;
        var month1 = document.getElementById('ctl00_ModuleContentPlaceHolder_ddlDate1Month').value;
        var year1 = document.getElementById('ctl00_ModuleContentPlaceHolder_txtDate1Year').value;
        
        var day2 = document.getElementById('ctl00_ModuleContentPlaceHolder_ddlDate2Day').value;
        var month2 = document.getElementById('ctl00_ModuleContentPlaceHolder_ddlDate2Month').value;
        var year2 = document.getElementById('ctl00_ModuleContentPlaceHolder_txtDate2Year').value;
        
        if ((StoryDate1Require(day1, month1, year1, 'Birthday', 'New Baby', 'Birthday', 'Memorial') == false) ||
            (StoryDate2Require(day2, month2, year2, 'Birthday', 'Memorial') == false))
        {
            args.IsValid = true;
        }
        else
        {
            var status = StoryCheckNewBaby(day1, month1, year1, day2, month2, year2,'Birthday', 'New Baby');
            var validator = document.getElementById('ctl00_ModuleContentPlaceHolder_valCheckNewBaby');
            
            if (status == 0)
            {
                validator.errormessage = "Date of Birth or Due Date is required";
                args.IsValid = false;
            }
            else if (status == 2)
            {
                
                validator.errormessage = "Please enter only a date of birth or a due date.";
                args.IsValid = false;
            }
            else if (status == 1)
            {
                args.IsValid = true;
            }
            
        }
    }
    
    function checkDate1(source, args)
    {   
        var day1 = Number(document.getElementById('ctl00_ModuleContentPlaceHolder_ddlDate1Day').value);
        var month1 = Number(document.getElementById('ctl00_ModuleContentPlaceHolder_ddlDate1Month').value);
        var year1 = document.getElementById('ctl00_ModuleContentPlaceHolder_txtDate1Year').value;
        
        var day2 = document.getElementById('ctl00_ModuleContentPlaceHolder_ddlDate2Day');
        var month2 = document.getElementById('ctl00_ModuleContentPlaceHolder_ddlDate2Month');
        var year2 = document.getElementById('ctl00_ModuleContentPlaceHolder_txtDate2Year');

        if (StoryDate1Require(day1, month1, year1, 'Birthday', 'New Baby', 'Birthday', 'Memorial') == false)
        {
            args.IsValid = true;
        }
        else
        {
            args.IsValid = StoryCheckDate1(day1, month1, year1, day2, month2, year2, 'Birthday', 'Memorial', 'New Baby', 'Birthday');
        }
    }

    function checkDate2(source, args)
    {
        var day1 = Number(document.getElementById('ctl00_ModuleContentPlaceHolder_ddlDate1Day').value);
        var month1 = Number(document.getElementById('ctl00_ModuleContentPlaceHolder_ddlDate1Month').value);
        var year1 = document.getElementById('ctl00_ModuleContentPlaceHolder_txtDate1Year').value;
        
