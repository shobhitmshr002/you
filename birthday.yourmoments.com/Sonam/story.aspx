        }
        else
        {
            args.IsValid = StoryCheckDate2(day1, month1, year1, day2, month2, year2, 'Birthday', 'New Baby');
        }
    }
    
    function checkFutureDate1(source, args)
    {
        var check = false;
        
        var day1 = Number(document.getElementById('ctl00_ModuleContentPlaceHolder_ddlDate1Day').value);
        var month1 = Number(document.getElementById('ctl00_ModuleContentPlaceHolder_ddlDate1Month').value);
        var year1 = document.getElementById('ctl00_ModuleContentPlaceHolder_txtDate1Year').value;
        
        if (('Birthday' == 'Memorial')|| ('Birthday' == 'New Baby'))
        {
            if ('Birthday' == 'New Baby')
            {
                var day2 = Number(document.getElementById('ctl00_ModuleContentPlaceHolder_ddlDate2Day').value);
                var month2 = Number(document.getElementById('ctl00_ModuleContentPlaceHolder_ddlDate2Month').value);
                var year2 = document.getElementById('ctl00_ModuleContentPlaceHolder_txtDate2Year').value;
                
                var status = StoryCheckNewBaby(day1, month1, year1, day2, month2, year2,'Birthday', 'New Baby');
                if ((status == 0) || (status == 2))
                {
                    check = false;
                }
                else if (status == 1)
                {
                    check = true;
                }
            }
            else
            {
                check = true;   
            }
            if(check == true)
            {
                var today = new Date( '2016', '1' - 1, '14' );
                args.IsValid = StoryCheckFutureDate( day1, month1, year1, today );
            }
            else
            {
                args.IsValid = true;
            }
        }
        else
        {
            args.IsValid = true;
        }
    }
    
    function checkFutureDate2(source, args)
    {
        if ('Birthday' == 'Memorial')
        {
            var today = new Date( '2016', '1' - 1, '14' );
        
            args.IsValid = StoryCheckFutureDate( Number(document.getElementById('ctl00_ModuleContentPlaceHolder_ddlDate2Day').value),
                                                 Number(document.getElementById('ctl00_ModuleContentPlaceHolder_ddlDate2Month').value),
                                                 document.getElementById('ctl00_ModuleContentPlaceHolder_txtDate2Year').value, today)
        }
        else
        {
            args.IsValid = true
        }
    }
    
    function checkDueDate(source, args)
